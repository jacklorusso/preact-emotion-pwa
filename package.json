{
  "name": "preact-emotion-pwa",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn run -s serve || yarn run -s dev",
    "build": "preact build",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "test": "eslint src && preact test",
    "format": "prettier --single-quote --trailing-comma es5 --write 'src/**/*.js'"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "devDependencies": {
    "eslint": "^4.6.1",
    "eslint-config-synacor": "^1.1.1",
    "if-env": "^1.0.0",
    "preact-cli": "^1.4.1",
    "prettier": "^1.6.1"
  },
  "dependencies": {
    "babel-plugin-emotion": "^7.2.2",
    "emotion": "^7.2.2",
    "preact": "^8.2.5",
    "preact-compat": "^3.17.0",
    "preact-emotion": "^7.2.2",
    "preact-router": "^2.5.7"
  }
}
